{
    "name": "avto-dev/monetaru-api-php",
    "description": "PHP package for a working with Moneta.ru service",
    "keywords": [
        "php",
        "moneta",
        "client",
        "api"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Avto Develop",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^5.6 || >=7.0 <7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.3",
        "nesbot/carbon": "~1.20",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.* || ~1.0",
        "phpunit/phpunit": "~5.7 || ^6.4 || ~7.0",
        "symfony/var-dumper": "~3.2 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\MonetaApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\MonetaApi\\Tests\\": "tests"
        }
    },
    "support": {
        "issues": "https://github.com/avto-dev/monetaru-api-php/issues",
        "source": "https://github.com/avto-dev/monetaru-api-php"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --no-coverage"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
